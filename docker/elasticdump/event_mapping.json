{
    "settings": {
      "index": {
        "refresh_interval": "1s",
        "number_of_shards": "1",
        "number_of_replicas": "1"
      }
    },
    "mappings": {
      "dynamic": "strict",
      "properties": {
        "id": {
          "type": "keyword"
        },
        "coefficient": {
          "type": "float"
        },
        "deadline": {
          "type": "date",
          "format": "epoch_second"
        },
        "state": {
          "type": "integer"
        }
      }
    }
}
